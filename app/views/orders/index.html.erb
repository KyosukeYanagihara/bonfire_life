<h2>注文履歴</h2>
<% @orders.each do |order| %>
<div>
  <%= order.created_at.strftime('%Y/%m/%d %H:%M:%S') %>
  <%= order.id %>
  <% order.order_items.each do |item| %>
    <%= item.product.name %>
  <% end %>
  <%= order.status %>
  <%= order.rental_start %>
  <%= order.rental_period %>
  <%= order.insurance_fee %>
  <%= order.price %>
</div>
<% end %>